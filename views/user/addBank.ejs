<%- include("../partials/header") %>
  <link rel="stylesheet" href="/stylesheets/addBank.css">

  <title>RMITdays</title>
</head>
<body>
  <div class="bank-header">
    <h1>My Bank Account</h1>
    <a href="bankManage">    <ion-icon class="bank-delete-icon" name="close-outline"></ion-icon></a>
  </div>
  <form id="addBank-form">
    <input type="text" id="cardname" placeholder="Card Name" required>
    <input type="text" id="cardNum" placeholder="Card Number" required>
    <input type="text" id="date" placeholder="MM/YY" required>
    <input type="text" id="cvv" placeholder="CVV">
  </form>
  <div id="form-btns">
    <input type="submit" id="save" value="Save">
    <input type="reset" id="reset" value="reset">
  </div>
</body>
</html>